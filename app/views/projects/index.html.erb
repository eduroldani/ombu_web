<div class="container">

<%@projects.each do |project| %>

  <ul class="shadow m-2">
    <h3>Title: <%= project.title %></h3>
      <%= project.description %>
      Level: <%= project.level %>
      Category: <%= project.category %>
       <%= link_to "Edit Project", edit_project_path(project.id), class: "" %>
  </ul>

<% end %>

  <%= link_to "Agregar Proyecto", new_project_path, class: "btn btn-secondary"  %>

</div>



